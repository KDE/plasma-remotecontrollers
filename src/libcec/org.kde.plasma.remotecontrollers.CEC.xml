<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN" "http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node>
  <interface name="org.kde.plasma.remotecontrollers.CEC">
    <signal name="enterStandby">
    </signal>
    <method name="sendNextKey">
      <arg type="i" direction="out"/>
    </method>
    <method name="hdmiCecSupported">
      <arg type="b" direction="out"/>
    </method>
    <method name="sendKey">
      <arg type="b" direction="out"/>
      <arg type="y" direction="in"/>
    </method>
    <method name="powerOnDevices">
      <arg type="b" direction="out"/>
    </method>
    <method name="powerOffDevices">
      <arg type="b" direction="out"/>
    </method>
    <method name="makeActiveSource">
      <arg type="b" direction="out"/>
    </method>
    <method name="setOSDName">
      <arg type="b" direction="out"/>
      <arg type="s" direction="in"/>
    </method>
  </interface>
</node>
